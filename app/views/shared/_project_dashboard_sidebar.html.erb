<div class="container text-center">
  <h2><%= @project.name %></h2>
  <% if !@project.client.nil? %>
    <h4>Client: <%= @project.client.email %></h4>
  <% end %>
  <% if @project.Completed? %>
    <%= render 'completed_summary', project: @project %>
  <% else %>
    <%= render 'project_navigation', project: @project %>
  <% end %>
  <!-- to do:
  Add summary for completed project
  disable buttons for completed projects
  add logic to show resume for paused projects -->



</div>
